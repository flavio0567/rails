
<h1>List all Dojos</h1>

<table class="table table-bordered table-striped">
<thead>
	<tr>
		<th>Coding Dojo Branch</th>
		<th>Street</th>
		<th>City</th>
		<th>State</th>
		<th>Actions</th>
	</tr>
</thead>
<tbody>

	<% @dojos.each do |dojo| %>
		<tr>
			<td><%= dojo.branch %></td>
			<td><%= dojo.street %></td>
			<td><%= dojo.city %></td>
			<td><%= dojo.state %></td>
			<td><a href="/dojos/<%= dojo.id %>">Show</a>
			    <a href="/dojos/<%= dojo.id %>/edit">Edit</a>
			    <a href="/dojos/<%= dojo.id %>">Destroy</a></td>
		</tr>
	<% end%>
</table>
<td><a href="dojos/new">New Dojo</a></td>
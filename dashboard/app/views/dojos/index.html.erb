<h1>List all Dojos</h1>
<table class="table table-bordered table-striped">
<thead>
	<tr>
		<th>Coding Dojo Branch</th>
		<th>Street</th>
		<th>City</th>
		<th>State</th>
	</tr>
</thead>
<tbody>

	<% @dojos.each do |dojo| %>
		<tr>
			<td><%= dojo.branch %></td>
			<td><%= dojo.street %></td>
			<td><%= dojo.city %></td>
			<td><%= dojo.state %></td>
		</tr>
	<% end%>
</table>